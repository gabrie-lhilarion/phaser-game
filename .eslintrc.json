{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-shadow": "off",
        "no-param-reassign": "off",
        "eol-last": "off",
        "import/no-extraneous-dependencies": "off",
        "no-unused-vars": "off",
        "no-new": "off"
    },
    "ignorePatterns": [
        "dist/",
        "build/",
        "test"
    ]
}
